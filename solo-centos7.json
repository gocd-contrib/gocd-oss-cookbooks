{
  "run_list": [

    "recipe[go-user]",
    "recipe[geckodriver]"
  ],
  "geckodriver": {
    "version": "v0.19.1"
  },
  "java": {
    "jdk_version": "8"
  },
  "tini": {
    "version": "v0.16.1"
  },
  "yum": {
    "rpmforge-extras": {
      "enabled": false,
      "gpgkey": "http://repository.it4i.cz/mirrors/repoforge/RPM-GPG-KEY.dag.txt"
    },
    "rpmforge-testing": {
      "enabled": false,
      "gpgkey": "http://repository.it4i.cz/mirrors/repoforge/RPM-GPG-KEY.dag.txt"
    },
    "rpmforge": {
      "enabled": false,
      "gpgkey": "http://repository.it4i.cz/mirrors/repoforge/RPM-GPG-KEY.dag.txt"
    }
  },
  "perforce": {
    "base_url": "https://s3.amazonaws.com/mirrors-archive/local/perforce/",
    "p4_version": "15.1",
    "p4d_version": "16.2"
  },
  "postgresql": {
    "enable_pgdg_yum": true,
    "pg_version": "9.6",
    "client": { "packages": [] }
  },
  "poise-python": {
    "options": {
      "wheel_version": false,
      "pip_version": "9.0.3"
    }
  },
  "cloudcli": {
    "aws": {
      "virtualenv": "/opt/local/awscli"
    }
  }
}
